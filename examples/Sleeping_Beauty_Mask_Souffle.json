  "processSteps": [
    {
      "id": "p1",
      "number": "1",
      "description": "In the main vessel, combine ingredients of Part A except Plantapon® LGC Sorb. Heat to 75°C and stir until uniform.",
      "equipment": "V-301",
      "parameters": [
        {
          "id": "param_temp_A",
          "name": "Temperature",
          "norm": "75°C"
        }
      ]
    },
    {
      "id": "p2",
      "number": "2",
      "description": "In the premix vessel, combine all ingredients of Part B. Heat to 75°C until all solids are melted.",
      "equipment": "V-302",
      "parameters": [
        {
          "id": "param_temp_B",
          "name": "Temperature",
          "norm": "75°C"
        }
      ]
    },
    {
      "id": "p3",
      "number": "3",
      "description": "Add Plantapon® LGC Sorb to Part A. Then, add Part B to Part A while stirring.",
      "equipment": "V-301",
      "parameters": []
    },
    {
      "id": "p4",
      "number": "4",
      "description": "Cool the emulsion to below 50°C. Add the components of Part C one by one, ensuring uniformity.",
      "equipment": "V-301",
      "parameters": [
        {
          "id": "param_cool",
          "name": "Cooling Temperature",
          "norm": "< 50°C"
        }
      ]
    },
    {
      "id": "p5",
      "number": "5",
      "description": "Add Part D (Tinovis® GTC UP) drop by drop and whip with a mixer to create the soufflé texture. Adjust the pH to 6.8 with Part E (NaOH solution) while mixing.",
      "equipment": "V-301, MIX-1, QC-PH-01",
      "parameters": [
        {
          "id": "param3",
          "name": "Target pH",
          "norm": "6.8"
        }
      ]
    }
  ],
