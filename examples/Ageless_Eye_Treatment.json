  "processSteps": [
    {
      "id": "p1",
      "number": "1",
      "description": "In the main vessel, add Rheocare® C Plus to water and stir until a totally homogeneous gel is built. Neutralize to pH 6.0-7.0 with Sodium Hydroxide (25% solution). This is Phase A.",
      "equipment": "V-201, QC-PH-02",
      "parameters": [
        {
          "id": "param_ph_A",
          "name": "Neutralization pH",
          "norm": "6.0 - 7.0"
        }
      ]
    },
    {
      "id": "p2",
      "number": "2",
      "description": "In a separate vessel, combine all ingredients of Phase B. Heat both Phase A and Phase B to 80°C separately.",
      "equipment": "V-201, V-202",
      "parameters": [
        {
          "id": "param_temp_AB",
          "name": "Temperature",
          "norm": "80°C"
        }
      ]
    },
    {
      "id": "p3",
      "number": "3",
      "description": "Add phase B to phase A under stirring.",
      "equipment": "V-201",
      "parameters": []
    },
    {
      "id": "p4",
      "number": "4",
      "description": "Begin cooling. At a temperature below 60°C, add phase C.",
      "equipment": "V-201",
      "parameters": [
        {
          "id": "param_temp_C",
          "name": "Addition Temperature",
          "norm": "< 60°C"
        }
      ]
    },
    {
      "id": "p5",
      "number": "5",
      "description": "Continue to cool down while stirring. At a temperature below 40°C, add ingredients of phase D to the combined phases (A+B+C) one by one.",
      "equipment": "V-201",
      "parameters": [
        {
          "id": "param_temp_D",
          "name": "Addition Temperature",
          "norm": "< 40°C"
        }
      ]
    },
    {
      "id": "p6",
      "number": "6",
      "description": "Adjust the final pH to 6.5 with phase E.",
      "equipment": "V-201, QC-PH-02",
      "parameters": [
        {
          "id": "param_ph_final",
          "name": "Target pH",
          "norm": "6.0 - 6.5"
        }
      ]
    }
  ],
